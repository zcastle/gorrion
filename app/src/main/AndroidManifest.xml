<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ww.gorrion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".LauncherActivity" />
        <activity android:name=".ProductoActivity" android:label="@string/title_activity_producto" />
        <activity android:name=".ProductoResultadoActivity"  android:label="@string/title_activity_producto_resultado" />
        <activity android:name=".ContactoActivity" android:label="@string/title_activity_contacto" />
        <activity android:name=".GuiaActivity" android:label="@string/title_activity_guia" />
    </application>

</manifest>